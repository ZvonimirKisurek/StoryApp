                android:id="@+id/buttonDay7Calendar"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:backgroundTint="#BFC5C5C5"
                android:gravity="center"
                android:text="7" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/buttonDay8Calendar"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:backgroundTint="#BFC5C5C5"
                android:gravity="center"
                android:text="8" />

            <Button
                android:id="@+id/buttonDay9Calendar"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:backgroundTint="#BFC5C5C5"
                android:gravity="center"
