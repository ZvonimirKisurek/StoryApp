            <Button
                android:id="@+id/buttonDay10Calendar"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:backgroundTint="#BFC5C5C5"
                android:gravity="center"
                android:text="10" />

            <Button
                android:id="@+id/buttonDay11Calendar"
                android:layout_width="45dp"
