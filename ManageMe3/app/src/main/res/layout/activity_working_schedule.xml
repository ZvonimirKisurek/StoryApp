                android:id="@+id/buttonDay1Calendar"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:backgroundTint="#BF2CC35E"
                android:gravity="center"
                android:text="1" />

            <Button
                android:id="@+id/buttonDay2Calendar"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:backgroundTint="#BFC5C5C5"
