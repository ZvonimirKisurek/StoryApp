        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/buttonDay29Calendar"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:backgroundTint="#BFC5C5C5"
                android:gravity="center"
                android:text="29" />

            <Button
                android:id="@+id/buttonDay30Calendar"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:backgroundTint="#BFC5C5C5"
                android:gravity="center"
                android:text="30" />

            <Button
                android:id="@+id/buttonDay31Calendar"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:backgroundTint="#BFF11F1F"
                android:gravity="center"
                android:text="31" />

        </TableRow>

    </TableLayout>

    <TextView
        android:id="@+id/textDatumCalendarWS"
        android:layout_width="325dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/circleblue"
        android:backgroundTint="#E663CDFD"
        android:fontFamily="@font/adamina"
        android:gravity="center"
        android:text="Siječanj 2023"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView48" />

    <ImageButton
        android:id="@+id/buttonIduciMjesecCalendarWS"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/tableLayout2"
        app:layout_constraintEnd_toEndOf="@+id/textDatumCalendarWS"
        app:layout_constraintTop_toTopOf="@+id/textDatumCalendarWS"
        app:srcCompat="@drawable/ic_baseline_arrow_circle_right_24"
        tools:srcCompat="@drawable/ic_baseline_arrow_circle_right_24" />

    <ImageButton
        android:id="@+id/buttonProsliMjesecCalendarWS"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:backgroundTint="#00FDFBFB"
        app:layout_constraintBottom_toBottomOf="@+id/textDatumCalendarWS"
        app:layout_constraintStart_toStartOf="@+id/textDatumCalendarWS"
        app:layout_constraintTop_toTopOf="@+id/textDatumCalendarWS"
        app:srcCompat="@drawable/ic_baseline_arrow_circle_left_24"
        tools:srcCompat="@drawable/ic_baseline_arrow_circle_left_24" />

    <LinearLayout
        android:id="@+id/clOdabirProstorijeWS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="@+id/textImeEtapeWS"
        app:layout_constraintStart_toStartOf="@+id/textImeEtapeWS"
        app:layout_constraintTop_toBottomOf="@+id/textImeEtapeWS">

        <TextView
            android:id="@+id/textProstor1WS"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/circletextblue"
            android:backgroundTint="#E663CDFD"
            android:fontFamily="@font/adamina"
            android:paddingStart="4dp"
            android:paddingEnd="4dp"
            android:text="Raspored rada"
            android:textColor="#FFFFFF"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textProstor2WS"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/circletextblue"
            android:backgroundTint="#E663CDFD"
            android:fontFamily="@font/adamina"
            android:paddingStart="4dp"
            android:paddingEnd="4dp"
            android:text="Stanje skladišta"
            android:textColor="#FFFFFF"
            android:textSize="20sp"
            android:visibility="invisible" />

    </LinearLayout>

    <TextView
        android:id="@+id/textCurrentTimeWS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/adamina"
        android:padding="4dp"
        android:text="Time"
        android:textColor="#E663CDFD"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageDropNameStageWS"
        android:layout_width="40dp"
        android:layout_height="0dp"
        android:background="@drawable/circletextblue"
        android:backgroundTint="#E663CDFD"
        android:scaleType="fitXY"
        android:src="@drawable/ic_baseline_arrow_drop_down_24"
        app:layout_constraintBottom_toBottomOf="@+id/textImeEtapeWS"
        app:layout_constraintStart_toEndOf="@+id/textImeEtapeWS"
        app:layout_constraintTop_toTopOf="@+id/textImeEtapeWS"
        tools:srcCompat="@drawable/ic_baseline_arrow_drop_down_24" />

    <TextView
        android:id="@+id/textImeEtapeWS"
        android:layout_width="wrap_content"
        android:layout_height="36dp"
        android:background="@drawable/circletextblue"
        android:backgroundTint="#E663CDFD"
        android:fontFamily="@font/adamina"
        android:padding="4dp"
        android:text="Raspored rada"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView48"
        app:layout_constraintStart_toEndOf="@+id/textView48"
        app:layout_constraintTop_toTopOf="@+id/textView48" />

    <TextView
        android:id="@+id/textView48"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/circletextblue"
        android:backgroundTint="#E663CDFD"
        android:fontFamily="@font/adamina"
        android:padding="4dp"
        android:text="Izaberite aktivnost: "
        android:textColor="#E6FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/llNatragWS" />

    <LinearLayout
        android:id="@+id/llNatragWS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:orientation="horizontal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="25dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            app:srcCompat="@drawable/ic_baseline_arrow_back_ios_24" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="@font/adamina"
            android:gravity="center"
            android:text="Natrag"
            android:textColor="#E663CDFD"
            android:textSize="20sp" />
    </LinearLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:id="@+id/clRadnoVrijemeWS"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/circleblueroomactivity"
        android:backgroundTint="#E663CDFD"
        android:padding="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableLayout2"
        app:layout_constraintVertical_bias="0.429">

        <LinearLayout
            android:id="@+id/linearLayout3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/textView51"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/adamina"
                android:text="Datum: "
                android:textSize="18sp"
                android:textColor="#FFF"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView52"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/adamina"
                android:text="1. siječnja 2023"
                android:textSize="18sp"
                android:textColor="#FFF"
                android:textStyle="bold" />
        </LinearLayout>

        <TextView
            android:id="@+id/textView53"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="4dp"
            android:fontFamily="@font/adamina"
            android:text="Početak radnog vremena:"
            android:textColor="#FFF"
            android:textStyle="bold"
            app:layout_constraintEnd_toStartOf="@+id/textView54"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout3" />

        <TextView
            android:id="@+id/textView54"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="4dp"
            android:fontFamily="@font/adamina"
            android:text="Kraj radnog vremena:"
            android:textColor="#FFF"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout3" />

        <TextView
            android:id="@+id/textView55"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/circleblueroomactivity"
            android:backgroundTint="#FFF"
            android:fontFamily="@font/adamina"
            android:padding="6dp"
            android:textStyle="bold"
            android:text="12:00"
            android:textColor="#E663CDFD"
            app:layout_constraintEnd_toEndOf="@+id/textView53"
            app:layout_constraintStart_toStartOf="@+id/textView53"
            app:layout_constraintTop_toBottomOf="@+id/textView53" />

        <TextView
            android:id="@+id/textView56"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/circleblueroomactivity"
            android:backgroundTint="#FFF"
            android:fontFamily="@font/adamina"
            android:padding="6dp"
            android:text="20:00"
            android:textStyle="bold"
            android:textColor="#E663CDFD"
            app:layout_constraintEnd_toEndOf="@+id/textView54"
            app:layout_constraintStart_toStartOf="@+id/textView54"
            app:layout_constraintTop_toBottomOf="@+id/textView54" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>