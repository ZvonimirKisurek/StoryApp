            <Button
                android:id="@+id/buttonDay12Calendar"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:backgroundTint="#BFC5C5C5"
                android:gravity="center"
                android:text="12" />

            <Button
                android:id="@+id/buttonDay13Calendar"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:backgroundTint="#BFC5C5C5"
                android:gravity="center"
                android:text="13" />

            <Button
                android:id="@+id/buttonDay14Calendar"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:backgroundTint="#BFC5C5C5"
                android:gravity="center"
                android:text="14" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/buttonDay15Calendar"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:backgroundTint="#BFC5C5C5"
                android:gravity="center"
                android:text="15" />

            <Button
                android:id="@+id/buttonDay16Calendar"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:backgroundTint="#BFC5C5C5"
                android:gravity="center"
                android:text="16" />

            <Button
                android:id="@+id/buttonDay17Calendar"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:backgroundTint="#BFC5C5C5"
                android:gravity="center"
                android:text="17" />

            <Button
                android:id="@+id/buttonDay18Calendar"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:backgroundTint="#BFC5C5C5"
                android:gravity="center"
                android:text="18" />

            <Button
                android:id="@+id/buttonDay19Calendar"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:backgroundTint="#BFC5C5C5"
                android:gravity="center"
                android:text="19" />

            <Button
                android:id="@+id/buttonDay20Calendar"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:backgroundTint="#BFC5C5C5"
                android:gravity="center"
                android:text="20" />

            <Button
                android:id="@+id/buttonDay21Calendar"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:backgroundTint="#BFC5C5C5"
                android:gravity="center"
                android:text="21" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/buttonDay22Calendar"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:backgroundTint="#BFC5C5C5"
                android:gravity="center"
                android:text="22" />

            <Button
                android:id="@+id/buttonDay23Calendar"
                android:layout_width="45dp"
                android:layout_height="match_parent"
                android:backgroundTint="#BFC5C5C5"
                android:gravity="center"
                android:text="23" />

            <Button
                android:id="@+id/buttonDay24Calendar"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:backgroundTint="#BFC5C5C5"
                android:gravity="center"
                android:text="24" />

            <Button
                android:id="@+id/buttonDay25Calendar"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:backgroundTint="#BFC5C5C5"
                android:gravity="center"
                android:text="25" />

            <Button
                android:id="@+id/buttonDay26Calendar"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:backgroundTint="#BFC5C5C5"
                android:gravity="center"
                android:text="26" />

            <Button
                android:id="@+id/buttonDay27Calendar"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:backgroundTint="#BFC5C5C5"
